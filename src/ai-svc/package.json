{
  "name": "ai-svc",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "dev:dapr": "dapr run --app-id ai-svc --app-port 3004 --dapr-http-port 3500 --dapr-grpc-port 50001 --placement-host-address localhost:50006 --scheduler-host-address localhost:50007 --components-path ../dapr/components.mcp -- npm run dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "init-stream": "node init-stream.cjs",
    "init-stream:k8s": "dotenv -e .env.k8s -- node init-stream.cjs",
    "reset-stream": "node reset-stream.cjs",
    "check-stream": "node check-stream.cjs",
    "check-stream:k8s": "dotenv -e .env.k8s -- node check-stream.cjs",
    "clean-consumers": "node clean-consumers.cjs",
    "check-consumers": "node check-consumers.cjs",
    "test": "vitest run",
    "test:unit": "vitest run src/services",
    "test:integration:readme-agent": "vitest run src/__tests__/integration/readme-agent-e2e.test.ts",
    "test:integration": "vitest run src/__tests__/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Event Service - Orchestrates player context data with enrichment from NATS and ClickHouse",
  "dependencies": {
    "@clickhouse/client": "^1.12.1",
    "@dapr/dapr": "^3.6.1",
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.3",
    "@effect/schema": "^0.75.5",
    "@langchain/core": "^0.3.23",
    "@langchain/langgraph": "^0.2.22",
    "@langchain/openai": "^0.3.17",
    "axios": "^1.12.2",
    "effect": "^3.18.4",
    "express": "^5.1.0",
    "openai": "^6.3.0",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "nats": "^2.29.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
